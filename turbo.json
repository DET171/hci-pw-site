{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"daemon": false,
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".keystone/**", "!.keystone/admin/.next/cache/**"]
		},
		"frontend#build": {
			"dependsOn": ["gql#build"],
			"inputs": ["$TURBO_DEFAULT$"],
			"outputs": ["dist/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"inputs": ["$TURBO_DEFAULT$"],
			"outputs": []
		},
		"//#format-lint": {},
		"//#format-lint:fix": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
