{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"daemon": false,
	"tasks": {
		"build": {
			"dependsOn": ["//#format-lint", "^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".keystone/", "build/"]
		},
		"start": {
			"dependsOn": ["^start"]
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"inputs": ["$TURBO_DEFAULT$"],
			"outputs": []
		},
		"//#format-lint": {},
		"//#format-lint:fix": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
